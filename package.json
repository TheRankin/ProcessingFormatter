{
  "name": "processingformatter",
  "displayName": "Processing Code Formatter",
  "description": "An unofficial code formatter for the Processing language",
  "license": "MIT",
  "icon": "icon.png",
  "version": "1.0.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/TheRankin/ProcessingFormatter"
  },
  "bugs": {
    "url": "https://github.com/TheRankin/ProcessingFormatter/issues"
  },
  "publisher": "TheRankin",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Formatters"
  ],
  "keywords": [
    "pde",
    "Processing"
  ],
  "activationEvents": [
    "onLanguage:pde"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "pde",
        "aliases": [
          "Processing",
          "processing",
          "pde"
        ],
        "extensions": [
          ".pde"
        ]
      }
    ],
    "documentFormattingEditProvider": [
      {
        "language": "pde",
        "provider": "processingformatter.formatCode"
      }
    ],
    "documentRangeFormattingEditProvider": [
      {
        "language": "pde",
        "provider": "processingformatter.formatCode"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.88.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}